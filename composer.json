{
    "name": "eltristi/extra-commands",
    "description": "A Laravel package to generate repositories, services, interfaces, traits and enums.",
    "type": "library",
    "require": {
        "illuminate/support": "^10.0",
        "php": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "eltristi\\ExtraCommands\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "eltristi\\ExtraCommands\\ExtraCommandsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}